        >
    <Grid>
        <Label IsHitTestVisible="False" Name="Top" Content="就绪" HorizontalAlignment="Left" Margin="80,40,0,0" VerticalAlignment="Top" Background="#4CFF0000"  FontSize="18" Foreground="Yellow"/>
        <Label IsHitTestVisible="False" Name="Jug" Content="就绪" HorizontalAlignment="Left" Margin="80,76,0,0" VerticalAlignment="Top" Background="#4CFF0000"  FontSize="18" Foreground="Yellow"/>
        <Label IsHitTestVisible="False" Name="Mid" Content="就绪" HorizontalAlignment="Left" Margin="80,112,0,0" VerticalAlignment="Top" Background="#4CFF0000"  FontSize="18" Foreground="Yellow"/>
        <Label IsHitTestVisible="False" Name="Bot" Content="就绪" HorizontalAlignment="Left" Margin="80,148,0,0" VerticalAlignment="Top" Background="#4CFF0000" FontSize="18" Foreground="Yellow"/>
        <Label IsHitTestVisible="False" Name="Sup" Content="就绪" HorizontalAlignment="Left" Margin="80,184,0,0" VerticalAlignment="Top" Background="#4CFF0000"  FontSize="18" Foreground="Yellow"/>
        <Label IsHitTestVisible="False" x:Name="Top_Copy" Content="上单" HorizontalAlignment="Left" Margin="34,40,0,0" VerticalAlignment="Top" Background="#4CFF0000"  FontSize="18" Foreground="Yellow"/>
